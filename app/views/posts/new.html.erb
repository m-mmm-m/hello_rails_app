<h1>新規投稿(new)</h1>
<p>Find me in app/views/posts/new.html.erb</p>
<%= form_with model: @post, local: true do |form| %>
    <div>
        <%= form.label :content, "内容" %>
        <%= form.text_area :content %>
    </div>
    <div>
        <%= form.submit "投稿する" %>
    </div>
<% end %>
<%= link_to "投稿一覧に戻る", posts_path %>